{
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft.AspNetCore": "Debug"
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://+:43000",
				"Protocols": "Http1AndHttp2"
			},
			"Https": {
				"Url": "https://+:43001",
				"Protocols": "Http1AndHttp2"
			},
			"Grpc": {
				"Url": "http://+:44000",
				"Protocols": "Http2"
			}
		}
	},
	"https_port": 43001,
	"Grpc": {
		"Port": 44000
	}
}
