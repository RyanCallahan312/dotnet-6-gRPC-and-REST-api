{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Information"
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://+:5000",
				"Protocols": "Http1AndHttp2"
			},
			"Https": {
				"Url": "https://+:5001",
				"Protocols": "Http1AndHttp2"
			},
			"Grpc": {
				"Url": "https://+:6001",
				"Protocols": "Http2"
			}
		}
	},
	"Grpc": {
		"Port": 6000
	}
}
