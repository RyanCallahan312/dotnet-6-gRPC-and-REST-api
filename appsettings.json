{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://+:5000",
				"Protocols": "Http1AndHttp2"
			},
			"Https": {
				"Url": "https://+:5001",
				"Protocols": "Http1AndHttp2"
			},
			"Grpc": {
				"Url": "http://+:6000",
				"Protocols": "Http2"
			},
			"Grpc-https": {
				"Url": "https://+:6001",
				"Protocols": "Http2"
			}
		}
	},
	"Grpc": {
		"HttpPort": 6000,
		"HttpsPort": 6001
	}
}
